@prefix dc:           <http://purl.org/dc/elements/1.1/> .
@prefix dcterms:      <http://purl.org/dc/terms/> .
@prefix geo:          <http://www.opengis.net/ont/geosparql#> .
@prefix gwml2:        <http://gwml2.org/def/gwml2#> .
@prefix hyf:          <https://www.opengis.net/def/schema/hy_features/hyf/> .
@prefix hyfo:         <http://hyfo.spatialai.org/v1/hyfo#> .
@prefix kwg-ont:      <http://stko-kwg.geog.ucsb.edu/lod/ontology/> .
@prefix nhdplusv2:    <http://nhdplusv2.spatialai.org/v1/nhdplusv2#> .
@prefix owl:          <http://www.w3.org/2002/07/owl#> .
@prefix quantitykind: <http://qudt.org/vocab/quantitykind/> .
@prefix qudt:         <http://qudt.org/schema/qudt/> .
@prefix rdf:          <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:         <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema:       <https://schema.org/> .
@prefix skos:         <http://www.w3.org/2004/02/skos/core#> .
@prefix spatial:      <http://purl.org/spatialai/spatial/spatial-full#> .
@prefix unit:         <http://qudt.org/vocab/unit/> .
@prefix wdt:          <https://www.wikidata.org/prop/direct/> .
@prefix xml:          <http://www.w3.org/XML/1998/namespace> .
@prefix xsd:          <http://www.w3.org/2001/XMLSchema#> .

<http://purl.org/spatialai/spatial/well-full> rdf:type owl:Ontology ;
                                              dcterms:contributor "Torsten Hahmann"@en, "David Kedrowski"@en, "Katrina Schweikert"@en ;
                                              dcterms:created "2025-01-22" ;
                                              dcterms:publisher "SAWGraph Project Team"@en ;
                                              dcterms:creator "David Kedrowski, SAWGraph Project"@en ;
                                              dcterms:description "This ontology supports querying the SAWGraph Knowledge Graph. It adapts and extends concepts from HyFO and GWML2"@en ;
											  dc:source "KWG Project: http://stko-kwg.geog.ucsb.edu/lod/ontology/" ;
                                              dcterms:title "SAWGraph's water well ontology - an adaptation and extension of concepts from HyFO and GWML2"@en ;
                                              owl:versionInfo "4.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
dcterms:contributor rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/created
dcterms:created rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/modified
dcterms:modified rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/creator
dcterms:creator rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/publisher
dcterms:publisher rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/description
dcterms:description rdf:type owl:AnnotationProperty .

###  http://purl.org/dc/terms/title
dcterms:title rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#description
skos:description rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#editorialNote
skos:editorialNote rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty .

###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .

###  https://schema.org/name
schema:name rdf:type rdf:Property ;
			rdfs:label "name" ;
			rdfs:comment "The name of the item." ;
			rdfs:subPropertyOf rdfs:label ;
			owl:equivalentProperty dcterms:title ;
			schema:domainIncludes schema:Thing ;
			schema:rangeIncludes schema:Text .


#################################################################
#    Object Properties
#################################################################

geo:defaultGeometry

geo:hasGeometry

hyfo:hasCasingDepth

hyfo:hasStaticWaterDepth

hyfo:hasTotalDepth

hyfo:hasYield

hyfo:hasBedrockDepth

hyfo:hasWellPurpose

hyfo:hasWellStatus

hyfo:hasWaterUse

hyfo:hasAquiferType

hyfo:tapsAquifer


#################################################################
#    Datatype Properties
#################################################################

###  http://hyfo.spatialai.org/v1/hyfo#hasId
hyfo:hasId rdf:type owl:DatatypeProperty ;
           rdfs:domain hyfo:WaterWell ;
		   rdfs:range xsd:string .

###  http://hyfo.spatialai.org/v1/hyfo#hasPrimaryId
hyfo:hasPrimaryId rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf hyfo:hasId .

###  http://hyfo.spatialai.org/v1/hyfo#hasPrimaryStateAgencyId
hyfo:hasPrimaryStateAgencyId rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf hyfo:hasPrimaryId ;
					  owl:disjointWith hyfo:hasPrimarySawgraphId .

###  http://hyfo.spatialai.org/v1/hyfo#hasPrimarySawgraphId
hyfo:hasPrimarySawgraphId rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf hyfo:hasPrimaryId ;
						  owl:disjointWith hyfo:hasPrimaryStateAgencyId .

###  http://hyfo.spatialai.org/v1/hyfo#hasSecondaryId
hyfo:hasSecondaryId rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf hyfo:hasId .

###  http://hyfo.spatialai.org/v1/hyfo#hasSecondaryStateAgencyId
hyfo:hasSecondaryStateAgencyId rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf hyfo:hasSecondaryId .

###  THESE NEED TO BE MOVED TO THEIR APPROPRIATE STATE-LEVEL ONTOLOGIES
###  https://sawgraph.spatialai.org/v1/me_mgs#hasWellNumber
me_mgs:hasWellNumber rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hyfo:hasPrimaryStateAgencyId .

###  https://sawgraph.spatialai.org/v1/me_mgs#hasWellCardNumber
me_mgs:hasWellCardNumber rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf hyfo:hasSecondaryStateAgencyId .

###  https://sawgraph.spatialai.org/v1/il_isgs#hasApiNumber
il_isgs:hasApiNumber rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf hyfo:hasPrimaryStateAgencyId .

###  https://sawgraph.spatialai.org/v1/il_isgs#hasIswsNumber
il_isgs:hasIswsNumber rdf:type owl:DatatypeProperty ;
                      rdfs:subPropertyOf hyfo:hasSecondaryStateAgencyId .

###  https://sawgraph.spatialai.org/v1/oh_odnr#hasSawgraphId
oh_odnr:hasSawgraphId rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf hyfo:hasPrimarySawgraphId .

###  https://sawgraph.spatialai.org/v1/ks_kgs#hasWellId
ks_kgs:hasWellId rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf hyfo:hasPrimaryStateAgencyId .

###  https://sawgraph.spatialai.org/v1/ks_kgs#hasWellKid
ks_kgs:hasWellKid rdf:type owl:DatatypeProperty ;
                  rdfs:subPropertyOf hyfo:hasSecondaryStateAgencyId .


